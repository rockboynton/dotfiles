[core]
    editor = code --wait --new-window
[diff]
    tool = vscode-difftool
[difftool "vscode-difftool"]
    cmd = code --wait --diff $LOCAL $REMOTE
[merge]
    tool = vscode-mergetool
[mergetool "vscode-mergetool"]
    cmd = code --wait $MERGED
